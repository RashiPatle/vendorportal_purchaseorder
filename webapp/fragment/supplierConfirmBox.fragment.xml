<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog id="supplierDialogId" title="Confirm" titleAlignment="Center">
        <content>
            <Text id="_IDGenText" text="{i18n>confirm}" textAlign="Center"/>
        </content>
        <beginButton>
            <Button id="_IDGenButton1" text="{i18n>accept}" type="Accept" press="onPressSave"/>
        </beginButton>
        <endButton>
            <Button id="_IDGenButton2" text="{i18n>reject}" type="Reject" press="onPressCancel"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>